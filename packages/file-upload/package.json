{
  "name": "@ayyaz-dev/file-upload",
  "version": "0.1.0",
  "type": "module",
  "description": "Full-stack file upload with presigned URLs. Supports Cloudflare R2, AWS S3. NestJS server + React client.",
  "keywords": [
    "file-upload",
    "r2",
    "cloudflare",
    "s3",
    "presigned-url",
    "nestjs",
    "react",
    "drag-drop"
  ],
  "author": "Ayyaz Zafar",
  "license": "MIT",
  "main": "./dist/shared/index.js",
  "types": "./dist/shared/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/shared/index.d.ts",
      "default": "./dist/shared/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/core": ">=10.0.0",
    "class-validator": ">=0.14.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    },
    "class-validator": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@types/react": "^19.1.6",
    "class-validator": "^0.14.3",
    "react": "^19.1.0",
    "typescript": "^5.7.2"
  }
}
